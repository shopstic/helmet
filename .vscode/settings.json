{
  "deno.enable": true,
  "[typescript]": {
    "editor.defaultFormatter": "denoland.vscode-deno",
    "editor.formatOnSave": true,
  },
  "[yaml]": {
    "editor.formatOnSave": false
  },
  "deno.import_intellisense_origins": {
    "https://deno.land": true
  },
  "files.watcherExclude": {
    "**/target": true
  },
}