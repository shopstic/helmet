{
  "name": "@wok/helmet",
  "version": "4.0.8",
  "exports": {
    ".": "./src/mod.ts",
    "./cli": "./src/cli.ts",
    "./deps/schema": "./src/deps/schema.ts",
    "./deps/k8s": "./src/deps/k8s.ts"
  },
  "publish": {
    "include": [
      "./src",
      "./deno.json",
      "./README.md",
      "./LICENSE"
    ],
    "exclude": [
      "**/*.test.ts"
    ]
  },
  "fmt": {
    "lineWidth": 120,
    "include": [
      "src",
      "examples"
    ]
  },
  "lint": {
    "include": [
      "src",
      "examples"
    ],
    "rules": {
      "exclude": [
        "no-slow-types"
      ]
    }
  },
  "imports": {
    "@std/assert": "jsr:@std/assert@^1.0.12",
    "@std/fmt": "jsr:@std/fmt@^1.0.6",
    "@std/fs": "jsr:@std/fs@^1.0.16",
    "@std/path": "jsr:@std/path@^1.0.8",
    "@std/semver": "jsr:@std/semver@^1.0.5",
    "@std/streams": "jsr:@std/streams@^1.0.9",
    "@std/yaml": "jsr:@std/yaml@^1.0.5",
    "@wok/case": "jsr:@wok/case@^1.0.2",
    "@wok/deup": "jsr:@wok/deup@^3.0.0",
    "@wok/k8s-api": "jsr:@wok/k8s-api@^1.32.2",
    "@wok/k8s-utils": "jsr:@wok/k8s-utils@^2.2.1",
    "@wok/schema": "jsr:@wok/schema@^3.0.0",
    "@wok/utils": "jsr:@wok/utils@^5.2.3",
    "json-schema-to-typescript": "npm:json-schema-to-typescript@^15.0.4"
  }
}
