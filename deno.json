{
  "name": "@wok/helmet",
  "version": "1.30.1",
  "exports": {
    ".": "./src/mod.ts",
    "./deps/typebox": "./src/deps/typebox.ts"
  },
  "publish": {
    "include": [
      "./src",
      "./deno.json",
      "./README.md",
      "./LICENSE"
    ]
  },
  "tasks": {
    "check_all": "deno check **/*.ts"
  },
  "fmt": {
    "lineWidth": 120,
    "include": [
      "src",
      "examples"
    ]
  },
  "lint": {
    "include": [
      "src",
      "examples"
    ],
    "rules": {
      "exclude": [
        "no-slow-types"
      ]
    }
  }
}
