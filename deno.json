{
  "name": "@wok/helmet",
  "version": "0.0.0",
  "exports": {
    ".": "./src/mod.ts",
    "./deps/typebox": "./src/deps/typebox.ts",
    "./deps/k8s": "./src/deps/k8s.ts"
  },
  "publish": {
    "include": ["./src", "./deno.json", "./README.md", "./LICENSE"],
    "exclude": ["**/*.test.ts"]
  },
  "tasks": {
    "check_all": "deno check **/*.ts"
  },
  "fmt": {
    "lineWidth": 120,
    "include": [
      "src",
      "examples"
    ]
  },
  "lint": {
    "include": [
      "src",
      "examples"
    ],
    "rules": {
      "exclude": ["no-slow-types"]
    }
  },
  "imports": {
    "@std/assert": "jsr:@std/assert@^1.0.2",
    "@std/fmt": "jsr:@std/fmt@^1.0.0",
    "@std/fs": "jsr:@std/fs@^1.0.1",
    "@std/io": "jsr:@std/io@^0.224.5",
    "@std/path": "jsr:@std/path@^1.0.2",
    "@std/semver": "jsr:@std/semver@^1.0.1",
    "@std/yaml": "jsr:@std/yaml@^1.0.3",
    "@wok/case": "jsr:@wok/case@^1.0.1",
    "@wok/typebox": "jsr:@wok/typebox@^0.32.36",
    "@wok/utils": "jsr:@wok/utils@^1.7.0"
  }
}
